{
	"schemaVersion": 1,
	"id": "polyworks",
	"version": "${version}",
	"name": "PolyWorks",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://wilux.net/",
		"sources": "https://github.com/wiluxgit/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/polyworks/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.wilux.PolyWorks"
		],
		"client": [
			"net.wilux.ExampleModClient"
		]
	},
	"mixins": [
		"polyworks.mixins.json",
		{
			"config": "polyworks.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.9",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*",
		"polymer-core": ">=0.7.7",
		"polymer-resource-pack": ">=0.7.7",
		"polymer-blocks": ">=0.7.7",
		"polymer-virtual-entity": ">=0.7.7"
	},
	"suggests": {
		"another-mod": "*"
	}
}